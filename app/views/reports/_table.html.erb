<div class="table">
  <table id="search-tracks-table">
    <thead>
      <tr>
        <% if @report.type == Report::SEARCH_OPTION %>
          <th>Cancion</th>
        <% else %>
          <th>Palabra</th>
        <% end %>
        <th>Frecuencia</th>
      </tr>
    </thead>
    <tbody>
      <% results.each do |result| %>
        <tr>
          <td><%= result.first %></td>
          <td><%= result.second %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>